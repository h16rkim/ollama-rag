{
  "name": "ollama-rag",
  "version": "1.0.0",
  "description": "Ollama Code Server with NestJS and ChromaDB integration",
  "main": "dist/main.js",
  "scripts": {
    "build": "nest build",
    "build:seed": "tsc --project tsconfig.seed.json",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "seed": "DIRECTORY_PATHS=~/develop/inflab/course-backend,~/develop/inflab/inflearn-backend,~/develop/inflab/search COLLECTION_NAME=code_farm node dist-seed/seed.js",
    "seed-optimized": "DIRECTORY_PATHS=~/develop/inflab/course-backend,~/develop/inflab/inflearn-backend,~/develop/inflab/search COLLECTION_NAME=code_farm node dist-seed/seed-optimized.js",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "axios": "^1.8.3",
    "chromadb": "^2.0.1",
    "cors": "^2.8.5",
    "express": "^4.18.3",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.20",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  }
}
